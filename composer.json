{
    "name": "drupaltest/behat-one-time-login",
    "description": "Authenticates users through the Drupal one time login mechanism.",
    "type": "library",
    "license": "GPL-2.0-or-later",
    "homepage": "https://github.com/drupaltest/behat-one-time-login",
    "authors": [
        {
            "name": "Pieter Frenssen",
            "role": "Maintainer"
        }
    ],
    "require": {
        "drupal/drupal-extension": "^4.1",
        "friends-of-behat/service-container-extension": "~1.0"
    },
    "require-dev": {
        "composer/installers": "~1.2",
        "drupal/core-composer-scaffold": "~8.8",
        "drupal/core-project-message": "~8.8",
        "drupal/core-recommended": "~8.8",
        "drush/drush": "~10.0",
        "squizlabs/php_codesniffer": "~3.5",
        "zaporylie/composer-drupal-optimizations": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "DrupalTest\\BehatOneTimeLogin\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"]
        }
    }
}
